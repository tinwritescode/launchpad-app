{
  "name": "database",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate --accept-data-loss --force-reset",
    "db:reset": "prisma migrate reset --force && npm run db:push && npm run db:seed",
    "db:seed": "esbuild prisma/seed.ts --outfile=node_modules/tmp-seed.cjs --bundle --format=cjs --external:prisma --external:@prisma/client && node node_modules/tmp-seed.cjs --preview-feature",
    "prisma:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "esbuild": "^0.17.14",
    "prisma": "^4.9.0",
    "typescript": "^5.0.4"
  }
}
