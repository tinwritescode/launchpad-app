{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "dependsOn": [
        "addTSNoCheck",
        "deployContract",
        "runLocalNode",
        "generateTypechain"
      ],
      "cache": false,
      "persistent": true
    },
    "deployContract": {
      "cache": false
    },
    "runLocalNode": {
      "cache": false
    },
    "generateTypechain": {
      "dependsOn": ["deployContract"]
    },
    "addTSNoCheck": {
      "dependsOn": ["generateTypechain", "deployContract"]
    }
  }
}
