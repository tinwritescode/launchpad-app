{
  "name": "launchpad-app",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "turbo": "dotenv -- turbo run"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest",
    "dotenv-cli": "^7.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.29.1"
}
